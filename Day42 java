package Coding100Hari;

public class Day42 {
    
    public static void main(String[] args) {
        int a = 4, b = 4, c = 2;
        Boolean d = a == b;
        Boolean e = b != c;
        System.out.println("Nilai a sama dengan nilai b : " + d);
        System.out.println("Nilai b tidak sama dengan nilai c : " + e);
    }
    
}
